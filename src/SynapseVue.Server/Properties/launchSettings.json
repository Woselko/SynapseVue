{
    "profiles": {
        "SynapseVue.Server": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://localhost:6030"
        },
        "SynapseVue.Server-Swagger": {
            "commandName": "Project",
            "launchBrowser": true,
            "launchUrl": "swagger",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "applicationUrl": "http://localhost:6030"
        },
        "SynapseVue.Server-BlazorWebAssembly": {
            "commandName": "Project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": true,
            "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
            "applicationUrl": "http://localhost:6030"
        },
        "WSL": {
            "commandName": "WSL2",
            "launchBrowser": true,
            "launchUrl": "http://localhost:6030/",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:6030"
            }
        },
        "Docker": {
            "commandName": "SdkContainer",
            "launchBrowser": true,
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/",
            "environmentVariables": {
                "ASPNETCORE_HTTP_PORTS": "6030"
            },
            "publishAllPorts": true,
            "useSSL": false,
            "httpPort": 6030
        }
    },
    "$schema": "http://json.schemastore.org/launchsettings.json"
}